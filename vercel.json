{
  "buildCommand": "cd OTP && npm run vercel-build",
  "outputDirectory": "OTP/dist/public",
  "installCommand": "cd OTP && npm ci",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[[...path]].ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}