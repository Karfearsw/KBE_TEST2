{
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install --production=false",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[[...path]].ts"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}